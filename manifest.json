{
  "manifest_version": 3,
  "name": "Gemini Cite Cleaner",
  "version": "1.0",
  "description": "Removes spurious [cite_start] tags from Gemini responses.",
  "content_scripts": [
    {
      "matches": [
        "*://gemini.google.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": []
}